{
  "name": "preact_cesium",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./src/index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "cp config/template.html src/ && export NODE_ENV=production && export ENABLE_BROTLI=true && preact build --config config/preact.config.js --no-esm --sw --brotli true --template ./src/template.html",
    "serve": "sirv build --cors --single --port 3000",
    "dev": "export PORT=3000 && preact watch --config config/preact.config.js",
    "lint": "eslint src",
    "test": "jest",
    "bundle_watch": "webpack-bundle-analyzer --port 4200 build/stats.json"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "assets-webpack-plugin": "^5.0.3",
    "chalk": "^4.1.0",
    "cjs": "^0.0.11",
    "compression-webpack-plugin": "^6.0.2",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.0.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs": "^0.0.1-security",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.4",
    "install": "^0.13.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "preact-cli": "^3.0.3",
    "preact-render-spy": "^1.2.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "raw-loader": "^4.0.2",
    "sass-loader": "^7.3.1",
    "serve": "^11.3.2",
    "sirv-cli": "^0.4.5",
    "strip-pragma-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.7",
    "url": "^0.11.0",
    "url-loader": "^4.1.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.0.9",
    "worker-loader": "^3.0.0",
    "zlib": "^1.0.5"
  },
  "dependencies": {
    "cesium": "^1.75.0",
    "netcdfjs": "^1.0.0",
    "preact": "^10.5.5",
    "preact-router": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
